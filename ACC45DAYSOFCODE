#include <iostream>
using namespace std;

int main() {

  int i,t, n;
  float arr[100];
  cin>>t;
  while(t--)
  {  cin >> n;
      cout << endl;
  for(i = 0; i < n; ++i) {
   arr[i]= i+1;
    cin >> arr[i];
  }

for(i = 1;i < n; ++i) {

    if(arr[0] < arr[i])
      arr[0] = arr[i];
  }

  cout<< arr[0]<<endl;}

  return 0;
}
